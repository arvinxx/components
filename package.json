{
  "name": "@arvinxu/components",
  "private": true,
  "author": "arvinxx(arvinx@foxmail.com)",
  "description": "空谷的组件库",
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/arvinxx/components#readme",
  "bugs": {
    "url": "https://github.com/arvinxx/components"
  },
  "repository": "git+https://github.com/arvinxx/components.git",
  "license": "MIT",
  "scripts": {
    "start": "yarn docs:dev",
    "prebuild": "npm run clean",
    "build": "father-build && yarn build:webpack",
    "build:webpack": "lerna run webpack",
    "c": "ts-node scripts/create-package",
    "---------": "",
    "release": "multi-semantic-release",
    "release:local": "multi-semantic-release --no-ci",
    "---------     ": "",
    "api:dev": "concurrently \"yarn api:watch\" \"yarn api:server\"",
    "api:build": "typedoc",
    "api:watch": "onchange 'packages/**/src/**/*.ts' -- npx typedoc",
    "api:server": "serve apis",
    "---------      ": "",
    "docs:dev": "umi dev --dumi",
    "docs:build": "umi build --dumi",
    "--------- ": "",
    "test": "lerna run test",
    "test:coverage": "jest --coverage",
    "---------   ": "",
    "check": "yarn check:lint && yarn check:circular && yarn check:types",
    "check:lint": "yarn lint-eslint",
    "check:circular": "dpdm packages/**/*.ts  --warning false  --tree false  --exit-code circular:1  -T true",
    "check:types": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .ts,.tsx --format=pretty ./packages",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "eslint --cache --ext .ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "---------    ": "",
    "clean": "lerna run clean && rm -rf es lib dist build coverage .umi .eslintcache apis",
    "reinstall": "yarn clean && lerna clean &&  rm -rf node_modules && yarn",
    "prepublishOnly": "yarn build",
    "prettier": "prettier -c --write \"**/**\"",
    "n": "ts-node"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": "npm run lint-staged:ts"
  },
  "devDependencies": {
    "@ant-design/pro-skeleton": "^1.0.0-beta.2",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.12.16",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@semantic-release/npm": "^7.1.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^13.0.8",
    "@types/classnames": "^2.2.7",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^15.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/uuid": "^8.3.0",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-dumi": "^1.1.9",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.10",
    "antd": "^4.14.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "canvas": "2.6.1",
    "commitlint": "^12.0.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^5.1.3",
    "dpdm": "^3.5.0",
    "ejs": "^3.1.6",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.2.1",
    "father-build": "^1.18.1",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.0.0",
    "jest": "^26.4.2",
    "lerna": "^4.0.0",
    "less-loader": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "multi-semantic-release": "^2.6.0",
    "onchange": "^7.1.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "prettier": "^2.1.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "semantic-release": "^17.4.7",
    "semantic-release-config-gitmoji": "^1.1.0",
    "serve": "^12.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "tplant": "^3.1.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.21.0",
    "typescript": "^4.2.4",
    "umi": "^3.4.2",
    "utility-types": "^3.10.0",
    "webpack": "^5.26.1",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
