{
  "name": "@arvinxu/components",
  "private": true,
  "author": "arvinxx(arvinx@foxmail.com)",
  "description": "空谷的组件库",
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/arvinxx/components#readme",
  "bugs": {
    "url": "https://github.com/arvinxx/components"
  },
  "repository": "git+https://github.com/arvinxx/components.git",
  "license": "MIT",
  "scripts": {
    "start": "pnpm run docs:dev",
    "prebuild": "pnpm run clean",
    "build": "pnpm run build:lib && pnpm run build:umd",
    "build:lib": "father-build",
    "build:umd": "lerna run webpack",
    "c": "ts-node scripts/create-package",
    "---------": "",
    "release": "multi-semantic-release",
    "release:local": "pnpm run build:lib && multi-semantic-release --no-ci",
    "---------     ": "",
    "api:dev": "concurrently \"pnpm run api:watch\" \"pnpm run api:server\"",
    "api:build": "typedoc",
    "api:watch": "onchange 'packages/**/src/**/*.ts' -- npx typedoc",
    "api:server": "serve apis",
    "---------      ": "",
    "docs:dev": "umi dev --dumi",
    "docs:build": "umi build --dumi",
    "postversion": "dumi assets",
    "--------- ": "",
    "test": "lerna run test",
    "test:update": "jest -u",
    "cov": "jest --coverage",
    "---------   ": "",
    "check": "pnpm run check:lint && pnpm run check:circular && pnpm run check:types",
    "check:lint": "pnpm run lint-eslint",
    "check:circular": "dpdm packages/**/*.ts  --warning false  --tree false  --exit-code circular:1  -T true",
    "check:types": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .ts,.tsx --format=pretty ./packages",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "eslint --cache --ext .ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "---------    ": "",
    "clean": "lerna run clean && rm -rf es lib dist build coverage .umi .eslintcache apis",
    "reinstall": "pnpm run clean && lerna clean &&  rm -rf node_modules && pnpm run",
    "prepublishOnly": "pnpm run build",
    "prettier": "prettier -c --write \"**/**\"",
    "n": "ts-node"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": "pnpm run lint-staged:ts"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@ant-design/pro-form": "1.55.0",
    "@ant-design/pro-skeleton": "1.0.5",
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@semantic-release/npm": "8.0.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "16.2.14",
    "@types/node": "15.14.9",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/uuid": "8.3.4",
    "@umijs/fabric": "2.10.2",
    "@umijs/preset-dumi": "1.1.38",
    "@umijs/preset-react": "1.8.30",
    "@umijs/test": "3.5.21",
    "antd": "4.18.8",
    "babel-loader": "8.2.3",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-module-resolver": "4.1.0",
    "canvas": "2.6.1",
    "commitlint": "12.1.4",
    "commitlint-config-gitmoji": "2.2.5",
    "concurrently": "6.5.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.7",
    "dayjs": "1.10.7",
    "dpdm": "3.8.0",
    "ejs": "3.1.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.32.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.5.0",
    "father-build": "1.20.0",
    "fs-extra": "10.0.1",
    "gh-pages": "3.2.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lerna": "4.0.0",
    "less-loader": "6.2.0",
    "lint-staged": "11.2.6",
    "mini-css-extract-plugin": "1.6.2",
    "multi-semantic-release": "2.11.1",
    "onchange": "7.1.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "prettier": "2.5.1",
    "progress-bar-webpack-plugin": "2.1.0",
    "semantic-release": "19.0.2",
    "semantic-release-config-gitmoji": "1.2.4",
    "serve": "12.0.1",
    "style-loader": "2.0.0",
    "stylelint": "13.13.1",
    "terser-webpack-plugin": "5.3.1",
    "tplant": "2.3.6",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typedoc": "0.21.10",
    "typescript": "4.5.5",
    "umi": "3.5.21",
    "utility-types": "3.10.0",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2"
  },
  "engines": {
    "node": ">=12"
  }
}
